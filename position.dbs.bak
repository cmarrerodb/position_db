		<view name="v_cant_imei_proveedores" >
			<comment><![CDATA[Cantidad de IMEIs por proveedores de servicio de GPS]]></comment>
			<view_script><![CDATA[CREATE VIEW v_cant_imei_proveedores AS
SELECT
	gps_proveedor.id AS proveedor_id, 
	gps_proveedor.razon_social AS proveedor, 
	COUNT(gps_imei.imei) AS cant_imei
FROM
	gps_imei
	LEFT JOIN
	gps_proveedor
	ON 
		gps_imei.proveedor_id = gps_proveedor.id
	LEFT JOIN
	trn_patentes
	ON 
		gps_imei.patente_id = trn_patentes.id
GROUP BY gps_proveedor.id, gps_proveedor.razon_social 
ORDER BY COUNT(gps_imei.imei) DESC;]]></view_script>
			<column name="proveedor_id" type="integer" />
			<column name="proveedor" type="char" />
			<column name="cant_imei" type="bigint" />
		</view>
